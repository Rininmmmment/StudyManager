<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MyPage</servlet-name>
  	<display-name>MyPage</display-name>
  	<description></description>
  	<servlet-class>servlet.MyPage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClinicService</servlet-name>
  	<display-name>ClinicService</display-name>
  	<description></description>
  	<servlet-class>servlet.ClinicService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShiftServlet</servlet-name>
  	<display-name>ShiftServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.ShiftServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShiftManegement</servlet-name>
  	<display-name>ShiftManegement</display-name>
  	<description></description>
  	<servlet-class>servlet.ShiftManegement</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyPage</servlet-name>
  	<url-pattern>/MyPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClinicService</servlet-name>
  	<url-pattern>/ClinicService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShiftServlet</servlet-name>
  	<url-pattern>/ShiftServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShiftManegement</servlet-name>
  	<url-pattern>/ShiftManegement</url-pattern>
  </servlet-mapping>
</web-app>
